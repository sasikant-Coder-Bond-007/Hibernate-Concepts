package com.example.pojo;

import javax.persistence.Column;
import javax.persistence.Embeddable;

@Embeddable
public class Address {
	
	@Column (name="City")
	private String city;
	@Column (name="Street")
	private String street;
	@Column (name="State")
	private String state;
	@Column (name="PinCode")
	private Long pincode;
}
